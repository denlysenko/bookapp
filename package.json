{
  "name": "bookapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:server:dev": "nx serve api",
    "start:server:testing": "ENV=testing nx serve api",
    "start:angular-desktop": "nx run-many --target=serve --projects=api,angular-desktop --parallel=true",
    "start:react-desktop": "nx run-many --target=serve --projects=api,react-desktop --parallel=true",
    "seed:db": "seed -u $(grep DB_URI .env.testing | cut -d '=' -f 2-) --drop-database ./seed-data",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint && ng lint",
    "e2e": "nx e2e",
    "e2e:angular-desktop": "nx e2e angular-desktop-e2e -- --watch=true",
    "e2e:react-desktop": "nx e2e react-desktop-e2e -- --watch=true",
    "start-server-and-test:angular-desktop": "start-server-and-test start:server:testing http-get://localhost:3333/ping e2e:angular-desktop",
    "start-server-and-test:react-desktop": "start-server-and-test start:server:testing http-get://localhost:3333/ping e2e:react-desktop",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "start.nativescript.mobile.ios": "cd apps/angular/mobile && ns run ios --device EF3DF515-D6E9-4E7F-8AE1-E77CA1A4157B --no-hmr",
    "start.nativescript.mobile.android": "cd apps/angular/mobile && ns run android --no-hmr",
    "prepare.nativescript.mobile.ios": "cd apps/angular/mobile && ns prepare ios --bundle --no-hmr --env.uglify --env.aot --env.production",
    "build.nativescript.mobile.android": "cd apps/angular/mobile && ns build android --bundle --no-hmr --env.uglify --env.aot --env.production",
    "clean.nativescript.mobile": "cd apps/angular/mobile && ns clean",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
